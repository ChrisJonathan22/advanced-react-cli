{
  "name": "advanced-react-cli",
  "version": "0.6.2",
  "description": "NPM package which aims to provide a better way to install packages for your react application through an intuitive CLI",
  "main": "cli.js",
  "scripts": {
    "react-cli": "node cli.js",
    "format": "prettier-standard '**/*.js' '*/**/*.jsx' '**/*.js'",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "minor-release": "npm version minor && npm publish && git push --follow-tags",
    "major-release": "npm version major && npm publish && git push --follow-tags"
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "prettier-standard",
        "git add"
      ],
      "**/*.jsx": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "bin": {
    "arc": "cli.js"
  },
  "keywords": [
    "react-cli",
    "react cli",
    "reactcli",
    "cli-react",
    "cli react",
    "advanced react cli",
    "advanced-react-cli"
  ],
  "author": "Dev Isle",
  "license": "GNU GPLv3",
  "dependencies": {
    "inquirer": "^6.4.1",
    "node-cmd": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devisle/advanced-react-cli.git"
  },
  "bugs": {
    "url": "https://github.com/devisle/advanced-react-cli/issues"
  },
  "homepage": "https://github.com/devisle/advanced-react-cli",
  "devDependencies": {
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "prettier-standard": "^9.1.1"
  }
}
